{
  "dashboard": {
    "id": null,
    "title": "Vehicle Valuation Engine: Production Dashboard",
    "panels": [
      {
        "type": "graph",
        "title": "Predicted vs Actual Price (MAE, RMSE)",
        "targets": [
          {"expr": "valuation_engine_mae", "legendFormat": "MAE"},
          {"expr": "valuation_engine_rmse", "legendFormat": "RMSE"}
        ],
        "datasource": "Prometheus",
        "gridPos": {"x": 0, "y": 0, "w": 24, "h": 8}
      },
      {
        "type": "stat",
        "title": "Prediction Count (Last 24h)",
        "targets": [
          {"expr": "valuation_engine_prediction_count", "legendFormat": "Predictions"}
        ],
        "datasource": "Prometheus",
        "gridPos": {"x": 0, "y": 8, "w": 8, "h": 8}
      },
      {
        "type": "stat",
        "title": "API Error Rate",
        "targets": [
          {"expr": "valuation_engine_api_error_rate", "legendFormat": "Error Rate"}
        ],
        "datasource": "Prometheus",
        "gridPos": {"x": 8, "y": 8, "w": 8, "h": 8}
      },
      {
        "type": "stat",
        "title": "Model Drift Score",
        "targets": [
          {"expr": "valuation_engine_model_drift", "legendFormat": "Drift Score"}
        ],
        "datasource": "Prometheus",
        "gridPos": {"x": 16, "y": 8, "w": 8, "h": 8}
      },
      {
        "type": "table",
        "title": "Recent Prediction Audit Log",
        "targets": [
          {"expr": "valuation_engine_audit_log", "legendFormat": "Audit Log"}
        ],
        "datasource": "Prometheus",
        "gridPos": {"x": 0, "y": 16, "w": 24, "h": 8}
      }
    ],
    "schemaVersion": 27,
    "version": 1,
    "refresh": "10s"
  }
}
