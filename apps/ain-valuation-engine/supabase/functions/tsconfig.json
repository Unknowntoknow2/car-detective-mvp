{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022", "DOM"],
    "module": "ES2022",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "noResolve": false,
    "types": ["./deno-types.d.ts"],
  "allowImportingTsExtensions": true,
  "noEmit": true
  },
  "include": ["**/*.ts", "./deno-types.d.ts", "**/*.test.ts"],
  "exclude": ["**/*.test.ts"]
}
